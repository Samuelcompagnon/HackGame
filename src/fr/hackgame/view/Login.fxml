<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" styleClass="background" stylesheets="@style-login.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.hackgame.view.LoginController">
   <children>
      <Label layoutX="219.0" layoutY="237.0" stylesheets="@style-login.css" text="Utilisateur" textFill="#251600" />
      <Label layoutX="184.0" layoutY="319.0" prefHeight="36.0" prefWidth="159.0" stylesheets="@style-login.css" text="Mot de passe" textFill="#251600">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="user_field" layoutX="357.0" layoutY="235.0" prefHeight="40.0" prefWidth="186.0" />
      <TextField fx:id="pass_field" layoutX="357.0" layoutY="317.0" prefHeight="40.0" prefWidth="186.0" />
      <Button layoutX="825.0" layoutY="551.0" mnemonicParsing="false" onAction="#handleCancel" text="Quitter" />
      <ButtonBar layoutX="321.0" layoutY="430.0" prefHeight="40.0" prefWidth="236.0">
        <buttons>
            <Button minWidth="43.0" mnemonicParsing="false" onAction="#handleValidate" prefHeight="25.0" prefWidth="121.0" text="Valider" />
          <Button minWidth="53.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="168.0" text="Nouveau" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
